<div class="form">
<form action="/login" method="post">
  <div class="imgcontainer">
    <span class="material-symbols-outlined">person</span>
    <div class="importance">
    <h2>Guest Login</h2></div>
  </div>
  <% if (typeof error !== 'undefined') { %>
    <p style="color: red;"><%= error %></p>
<% } %>
  <div class="container">
    <label for="username"><b>Username</b></label>
    <input type="text" placeholder="Enter Username" name="username" required>    

    <label for="pwd"><b>Password</b></label>
    <input type="password" placeholder="Enter Password" name="pwd" id="password" required>
    
    <button type="submit" onclick="trimPassword()">Login</button>
  </div>

  <div class="container">
    <button type="button" class="cancelbtn" onclick="goToHomepage()">Cancel</button>
    <span class="psw"><a href="/signup">New Registeration</a></span>
  </div>
