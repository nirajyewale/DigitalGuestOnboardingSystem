    <h1>Registered Hotels</h1>
    <a href="/hotel/add">Add New Hotel</a>
    <table>
        <thead>
            <tr>
                <th>Hotel Name</th>
                <th>Address</th>
                <th>Logo</th>
                <th>QR Code</th>
            </tr>
        </thead>
        <tbody>
            <% hotels.forEach(hotel => { %>
                <tr>
                    <td><%= hotel.name %></td>
                    <td><%= hotel.address %></td>
                    <td><img src="<%= hotel.logo %>" alt="Hotel Logo" width="100"></td>
                    <td><img src="<%= hotel.qrCode %>" alt="QR Code" width="100"></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
